<div class="page-header">
  <h1>User #<%= @user.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>First name</dt>
  <dd><%= @user.first_name %></dd>

  <dt>Last name</dt>
  <dd><%= @user.last_name %></dd>

  <dt>Zip code</dt>
  <dd><%= @user.zip_code %></dd>

  <dt>Email</dt>
  <dd><%= @user.email %></dd>

  <dt>Phone number</dt>
  <dd><%= @user.phone_number %></dd>

</dl>
